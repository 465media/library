      "vendor/meta-box/meta-box-facetwp-integrator/": ["wpackagist-plugin/meta-box-facetwp-integrator"],
      "vendor/meta-box/meta-box-yoast-seo/": ["wpackagist-plugin/meta-box-yoast-seo"],
      "vendor/meta-box/meta-box-text-limiter/": ["wpackagist-plugin/meta-box-text-limiter"],
      "vendor/meta-box/{$name}/": ["vendor:meta-box"]
    }
  },
  "autoload": {
    "files": [
      "vendor/meta-box/meta-box/meta-box.php",
      "vendor/meta-box/mb-rest-api/mb-rest-api.php",
      "vendor/meta-box/mb-relationships/mb-relationships.php",
      "vendor/meta-box/mb-comment-meta/mb-comment-meta.php",
      "vendor/meta-box/mb-custom-post-type/mb-custom-post-type.php",
      "vendor/meta-box/meta-box-beaver-themer-integrator/meta-box-beaver-themer-integrator.php",
